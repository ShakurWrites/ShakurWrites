    <span class="hljs-attribute">margin</span>: <span class="hljs-number">0</span> auto <span class="hljs-number">20px</span> auto;
}

<span class="hljs-comment">/* Navigation Menu */</span>
<span class="hljs-selector-tag">nav</span> {
